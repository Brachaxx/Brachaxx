using System;

class Program
{
    static void Main()
    {
        Console.Write("Podaj liczbę, dla której chcesz wyświetlić tabliczkę mnożenia: ");
        int number = Convert.ToInt32(Console.ReadLine());
        
        for (int i = 1; i <= 10; i++)
        {
            Console.WriteLine(number + " * " + i + " = " + (number * i));
        }
    }
}
