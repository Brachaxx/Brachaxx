using System;

class Program
{
    static void Main()
    {
        Console.Write("Podaj sw√≥j rok urodzenia: ");
        int yearOfBirth = Convert.ToInt32(Console.ReadLine());
        
        int currentYear = DateTime.Now.Year;
        int age = currentYear - yearOfBirth;
        
        Console.WriteLine("Masz " + age + " lat.");
    }
}
